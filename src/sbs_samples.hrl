<<"SELECT ENPLASS_KEY, ENPLASS_ENPLAID, ENPLASS_INSYNC, ENPLASS_SYNC_RETRIES,
ENPLASS_SYNC_DATE, ENPLASS_SYNC_REQUEST_DATE, ENPLASS_ENABLER_ACTIVE
FROM
ENPLASYNCSTATE;">>,
<<"SELECT ENPLA_ID, ENPLA_ALIASID, ENPLA_TYPEID, ENPLA_SYNCHRONIZE, ENPLA_REFRESH,
ENPLA_REFRESH_DATE, ENPLA_NEXT_REF_BLOCK, ENPLA_TEST_INSTANCE,
ENPLA_INSTANCE_ID, ENPLA_INSTANCE_COUNT, ENPLA_THREAD_COUNT, ENPLA_DEBUG,
ENPLA_LOGIN, ENPLA_PWD
FROM ENPLATFORM;">>,
<<"SELECT HIH_ID, HIH_SRCTYPE, HIH_MAPID, HIH_DATETIME, HIH_FILENAME,
