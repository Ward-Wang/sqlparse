FROM SBS0_ADMIN.HIHEADER
WHERE HIH_DATETIME >=
sysdate-1/144 AND HIH_MAPID LIKE 'MB-MO%'
ORDER BY HIH_START;">>,
<<"SELECT HIH_ID, HIH_SRCTYPE, HIH_MAPID, HIH_FILESEQ, HIH_DATETIME, HIH_FILENAME,
