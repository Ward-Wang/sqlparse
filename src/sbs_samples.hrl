FROM SBS0_ADMIN.HIHEADER
WHERE HIH_DATETIME >= sysdate-1/24
AND
HIH_SRCTYPE IN ('MBS')
ORDER BY HIH_START;">>,
<<"SELECT HIH_ID, HIH_SRCTYPE, HIH_MAPID, HIH_DATETIME, HIH_FILENAME,
HIH_RECCOUNT, HIH_ERRCOUNT, HIH_ESID, HIH_DATEFC, HIH_DATELC, HIH_RECCOUNT /
((HIH_END-HIH_START+0.0000001)*24*3600) 'Speed', (HIH_END-HIH_DATEFC)*24*60
'Delay', HIH_JOB, HIH_HOST, HIH_VERSION
FROM SBS0_ADMIN.HIHEADER
WHERE
