ENPLALINK
WHERE ENPLATFORM.ENPLA_ID = ENPLALINK.ENPLAL_ENPLAID AND ENPLA_LOGIN
Is Not NULL
ORDER BY ENPLA_ID;">>,
<<"SELECT ENPLASS_ENPLAID, COUNT(*)
FROM ENPLASYNCSTATE
WHERE ENPLASS_INSYNC =
0
GROUP BY ENPLASS_ENPLAID;">>,
<<"SELECT ENPLASE_KEY, ENPLASE_ENPLAID, ENPLASE_ERRORTIME, ENPLASE_ERRTID,
ENPLASE_TOPIC, ENPLASE_MESSAGE, ENPLASE_CLOSETIME
FROM ENPLASYNCERROR
ORDER
BY ENPLASE_ERRORTIME DESC;">>,
<<"SELECT ENPLASS_ENPLAID, TRUNC(ENPLASS_SYNC_DATE,'HH24'), COUNT(*), COUNT(*)/60,
COUNT(*)/3600
FROM ENPLASYNCSTATE
WHERE ENPLASS_SYNC_DATE >=
trunc(sysdate,'HH24') - 1 AND ENPLASS_SYNC_DATE <  trunc(sysdate,'HH24')
GROUP
BY ENPLASS_ENPLAID, trunc(ENPLASS_SYNC_DATE,'HH24')
ORDER BY
ENPLASS_ENPLAID,
2;">>,
<<"SELECT ENPLASS_ENPLAID, TRUNC(ENPLASS_SYNC_DATE,'MI'), COUNT(*),
COUNT(*)/60
FROM ENPLASYNCSTATE
WHERE ENPLASS_SYNC_DATE >=
trunc(sysdate,'MI') - 1/24 AND ENPLASS_SYNC_DATE <  trunc(sysdate,'MI')
GROUP
BY ENPLASS_ENPLAID, trunc(ENPLASS_SYNC_DATE,'MI')
ORDER BY ENPLASS_ENPLAID;">>,
<<"SELECT ENPLASS_KEY, ENPLASS_ENPLAID, ENPLASS_INSYNC, ENPLASS_SYNC_DATE,
ENPLASS_SYNC_RETRIES, ENPLASS_REFRESH_DATE, ENPLASS_SYNC_REQUEST_DATE,
ENPLASS_ENABLER_ACTIVE, ENPLASS_MASTER_ACTIVE, ENPLASS_ENABLER_HASH,
ENPLASS_MASTER_HASH, ENPLASS_STATECOUNT
FROM ENPLASYNCSTATE
WHERE
ENPLASS_SYNC_DATE < sysdate AND ENPLASS_SYNC_DATE >= sysdate - 1/24;">>,
<<"SELECT ENPLASS_ENPLAID, ENPLASS_INSYNC,
DECODE(ENPLASS_SYNC_DATE,NULL,'NULL','SYNCED'), ENPLASS_ENABLER_ACTIVE,
COUNT(*)
