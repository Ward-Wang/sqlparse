FROM SBS0_ADMIN.HIMISSING
WHERE HIM_DATETIME >=
trunc(sysdate)-4;">>,
<<"SELECT HIH_ID, HIH_SRCTYPE, HIH_MAPID, HIH_DATETIME, HIH_FILENAME,
