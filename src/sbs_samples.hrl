sysdate-1/24 AND HIH_SRCTYPE IN ('ISRV')
ORDER BY HIH_START;">>,
<<"SELECT HIH_ID, HIH_SRCTYPE, HIH_MAPID, HIH_FILESEQ, HIH_DATETIME, HIH_FILENAME,
HIH_RECCOUNT, HIH_ERRCOUNT, HIH_ESID, HIH_DATEFC, HIH_DATELC, HIH_RECCOUNT /
