-define(SBS_SAMPLES, [
<<"SELECT SIC_MSISDN, SIC_IMSI, SIC_PAYMENT_METHOD, SIC_STYPE, SIC_DUOCARD,
SIC_DUAL_MSISDN, SIC_DUAL_IMSI, SIC_ENTRY_DATE, SIC_VALID_DATE,
SIC_TRANSFER_DATE, SIC_CHANGE_DATE, SIC_SYNC_DATE, SIC_ESID, SIC_STATUS,
SIC_PAYMENT_TYPE, SIC_USER_LANGUAGE, SIC_BIRTHDATE, SIC_MAIN_SEGMENT,
SIC_BIRTHDATE_STATUS, SIC_BARRING_STATE, SIC_CUSTOMER_ID, SIC_COMPANY_ID,
SIC_TOP_LEVEL_COMPANY_ID, SIC_FINE_SEGMENT, SIC_SERVICE_NUMBER, SIC_BRAND,
SIC_MODEL, SIC_REFRESH_TYPE, SIC_CHANGECOUNT
FROM SBS0_ADMIN.SICACHE
WHERE
SIC_PAYMENT_METHOD = 5 AND SIC_ENTRY_DATE >= sysdate -1/96
ORDER BY
SIC_ENTRY_DATE, SIC_ENTRY_DATE, SIC_ENTRY_DATE DESC, SIC_ENTRY_DATE DESC,
SIC_ENTRY_DATE, SIC_ENTRY_DATE DESC, SIC_ENTRY_DATE, SIC_ENTRY_DATE DESC;">>,
<<"SELECT SADDR, SID, SERIAL#, SCHEMANAME, OSUSER, PROCESS, MACHINE, PROGRAM,
LOGON_TIME
FROM \"SYS.V_$SESSION\"
WHERE SCHEMANAME IN ('SBS0_AAAPROVWS')
ORDER
BY MACHINE, LOGON_TIME;">>,
<<"SELECT AAAT_MSISDN, AAAT_TOPSCM, AAAT_TOPCU, AAAT_CHARGED, AAAT_BARSCM,
AAAT_BARCU, AAAT_AAASPID, AAAT_SYNCDATE, AAAT_ESID, AAAT_ACIDCRE, AAAT_DATECRE,
AAAT_ACIDMOD, AAAT_DATEMOD, AAAT_CHNGCNT, AAAT_PREPAID, AAAT_EXTID,
AAAT_SC
FROM SBS0_ADMIN.AAATRACKING
WHERE AAAT_SYNCDATE >= sysdate -1/24 AND
AAAT_ACIDCRE = 'CCPROV'
ORDER BY AAAT_SYNCDATE, AAAT_SYNCDATE DESC;">>,
<<"SELECT MACHINE, COUNT(*)
FROM SYS.V_$SESSION
WHERE USERNAME IN
('SBS0_AAAPROVWS')
GROUP BY MACHINE
ORDER BY MACHINE;">>,
<<"SELECT SICPM_ID, SICPM_CODE, SICPM_LANG01, SICPM_LANG02, SICPM_LANG03,
SICPM_LANG04, SICPM_VALUE1, SICPM_VALUE2, SICPM_VALUE3, SICPM_VALUE4,
SICPM_PREPAID
FROM SBS0_ADMIN.SICPM;">>,
<<"SELECT AAAT_MSISDN, AAAT_TOPSCM, AAAT_TOPCU, AAAT_CHARGED, AAAT_BARSCM,
AAAT_BARCU, AAAT_AAASPID, AAAT_SYNCDATE, AAAT_ESID, AAAT_ACIDCRE, AAAT_DATECRE,
AAAT_ACIDMOD, AAAT_DATEMOD, AAAT_CHNGCNT, AAAT_PREPAID, AAAT_EXTID, AAAT_SC,
AAAT_ABOSTARTDATE, AAAT_ABOSTARTNOTSTATE
FROM SBS0_ADMIN.AAATRACKING
WHERE
ROWNUM <= 100 AND AAAT_TOPSCM >= 500.0000 AND AAAT_CHARGED >= 500.0000
ORDER
BY AAAT_SYNCDATE DESC;">>,
<<"SELECT A.AAAT_MSISDN, A.AAAT_TOPSCM, A.AAAT_TOPCU, A.AAAT_CHARGED,
A.AAAT_BARSCM, A.AAAT_BARCU, A.AAAT_AAASPID, A.AAAT_SYNCDATE, A.AAAT_ESID,
A.AAAT_ACIDCRE, A.AAAT_DATECRE, A.AAAT_ACIDMOD, A.AAAT_DATEMOD, A.AAAT_CHNGCNT,
A.AAAT_PREPAID, A.AAAT_EXTID, A.AAAT_SC
FROM AAATRACKING A
WHERE AAAT_MSISDN
= '41792256540';">>,
<<"SELECT 'ADD PP_USER 00' || ENPLAPK_KEY || ' /PP_STATUS=UNBLOCKED'
FROM
ENPLAPROTECTEDKEY
WHERE ENPLAPK_ENPLAID IN ('SMCH80');">>,
<<"SELECT 'ADD PP_USER 00' || (41798900000 + ROWNUM) ||
' /PP_STATUS=UNBLOCKED'
FROM SICACHE
WHERE ROWNUM <= 999;">>,
<<"SELECT TABLE_NAME, INDEX_NAME, COLUMN_NAME
FROM ALL_IND_COLUMNS
WHERE
INDEX_OWNER = 'SBS0'
ORDER BY TABLE_NAME, INDEX_NAME, COLUMN_POSITION;">>,
<<"SELECT TABLE_NAME, INDEX_NAME, TABLESPACE_NAME, INI_TRANS, MAX_TRANS,
INITIAL_EXTENT, NEXT_EXTENT, BLEVEL, LEAF_BLOCKS, DISTINCT_KEYS,
AVG_LEAF_BLOCKS_PER_KEY, AVG_DATA_BLOCKS_PER_KEY, CLUSTERING_FACTOR, NUM_ROWS,
SAMPLE_SIZE, LAST_ANALYZED
FROM ALL_INDEXES
WHERE OWNER = 'SBS0';">>,
<<"SELECT TABLE_NAME, TABLESPACE_NAME, INITIAL_EXTENT, NEXT_EXTENT, NUM_ROWS,
BLOCKS, EMPTY_BLOCKS, AVG_SPACE, CHAIN_CNT, AVG_ROW_LEN,
AVG_SPACE_FREELIST_BLOCKS, NUM_FREELIST_BLOCKS, SAMPLE_SIZE,
LAST_ANALYZED
FROM ALL_TABLES
WHERE OWNER = 'SBS0' AND TABLESPACE_NAME Is Not
NULL;">>,
<<"SELECT QUEUE, CONSUMER_NAME, MSG_STATE, RETRY_COUNT, COUNT(*)
FROM
AQ_CONTROL
GROUP BY queue,
	consumer_name,
	msg_state,
	retry_count;">>,
<<"SELECT MSG_STATE, MIN(ENQ_TIME), MAX(ENQ_TIME), COUNT(*)
FROM
SBS0_ADMIN.AQ$SISNOT_AQ_TABLE A
GROUP BY msg_state
ORDER BY MSG_STATE;">>,
<<"SELECT MSG_STATE, MIN(ENQ_TIME), MAX(ENQ_TIME), COUNT(*)
FROM
SBS0_ADMIN.AQ$STAN_AQ_TABLE A
GROUP BY msg_state
ORDER BY MSG_STATE;">>,
<<"SELECT MSG_STATE, MIN(ENQ_TIME), MAX(ENQ_TIME), COUNT(*)
FROM
SBS0_ADMIN.AQ$SUBNOTIN_AQ_TABLE A
GROUP BY msg_state
ORDER BY MSG_STATE;">>,
<<"SELECT MSG_STATE, MIN(ENQ_TIME), MAX(ENQ_TIME), COUNT(*)
FROM
SBS0_ADMIN.AQ$SIS_AQ_TABLE A
GROUP BY msg_state
ORDER BY MSG_STATE;">>,
<<"SELECT BIH_ID, BIH_SRCTYPE, BIH_MAPID, BIH_FILESEQ, BIH_DATETIME, BIH_FILENAME,
BIH_RECCOUNT, BIH_ESID, BIH_DATEFC, BIH_DATELC, BIH_RECCOUNT /
((BIH_END-BIH_START+0.0000001)*24*3600) 'Speed'
FROM
SBS0_ADMIN.BIHEADER
WHERE BIH_DATETIME >= sysdate-15 AND BIH_MAPID IN
('AAABULK')
ORDER BY BIH_START;">>,
<<"SELECT BIH_ID, BIH_SRCTYPE, BIH_MAPID, BIH_FILESEQ, BIH_DATETIME, BIH_FILENAME,
BIH_RECCOUNT, BIH_ESID, BIH_DATEFC, BIH_DATELC, BIH_RECCOUNT /
((BIH_END-BIH_START+0.0000001)*24*3600) 'Speed'
FROM SBS0_ADMIN.BIHEADER
WHERE
BIH_DATETIME >= sysdate-24/24
ORDER BY BIH_START;">>,
<<"SELECT BIVAL_BILLTEXT, BIVAL_SHORTID, BIVAL_REPID, BIVAL_REASON,
BIVAL_DATE
FROM BILLTEXT_VALIDATION
WHERE BIVAL_BILLTEXT NOT LIKE
'%polyphonic%' AND BIVAL_BILLTEXT NOT LIKE '%]%' AND BIVAL_SHORTID LIKE
'6%'
ORDER BY BIVAL_DATE DESC;">>,
<<"SELECT BIVAL_SHORTID, BIVAL_BILLTEXT, BIVAL_REASON FROM
SBS0_ADMIN.BILLTEXT_VALIDATION WHERE  BIVAL_REPID = 'STANDARD' AND BIVAL_DATE >=
to_date('22.12.2006','dd.mm.yyyy') ORDER BY to_number(BIVAL_SHORTID),
BIVAL_REASON, BIVAL_BILLTEXT;">>,
<<"SELECT BIVAL_SHORTID, BIVAL_BILLTEXT FROM SBS0_ADMIN.BILLTEXT_VALIDATION WHERE 
BIVAL_REPID Is NULL AND BIVAL_SHORTID Not In
('898','959','987','9090','9123','9229','69569') AND BIVAL_DATE >=
to_date('22.12.2006','dd.mm.yyyy') ORDER BY to_number(BIVAL_SHORTID);">>,
<<"SELECT BIVAL_BILLTEXT, BIVAL_SHORTID, BIVAL_REPID, BIVAL_REASON,
BIVAL_DATE
FROM SBS0_ADMIN.BILLTEXT_VALIDATION
WHERE BIVAL_DATE > sysdate-10;">>,
<<"SELECT BOH_PACKID, BOH_FILENAME, COUNT(*), MIN(BOH_DATETIME),
MAX(BOH_DATETIME)
FROM SBS0_ADMIN.BOHEADER
WHERE BOH_DATETIME <
trunc(sysdate,'MONTH') AND BOH_DATETIME >=
add_months(trunc(sysdate,'MONTH'),-1)
GROUP BY BOH_PACKID, BOH_FILENAME;">>,
<<"SELECT BOH_PACKID, BOH_FILENAME, COUNT(*), MIN(BOH_DATETIME),
MAX(BOH_DATETIME)
FROM SBS0_ADMIN.BOHEADER
WHERE BOH_DATETIME < sysdate AND
BOH_DATETIME >= sysdate-8/24
GROUP BY BOH_PACKID, BOH_FILENAME;">>,
<<"SELECT BOH_ID, BOH_PACKID, BOH_FILESEQ, BOH_DATETIME, BOH_FILENAME,
BOH_RECCOUNT, BOH_DATEFC, BOH_DATELC, BOH_RECCOUNT / ((
BOH_END-BOH_START+0.000001)*24*3600) 'Speed', BOH_ESID
FROM
SBS0_ADMIN.BOHEADER
WHERE BOH_DATETIME > sysdate - 1 AND BOH_FILENAME LIKE
'SP_MECCACHE_%_AGE';">>,
<<"SELECT BOH_ID, BOH_DEMO, BOH_FILESEQ, BOH_FILEFORM, BOH_DATETIME, BOH_FILENAME,
BOH_FILEDATE, BOH_RECCOUNT, BOH_PACKID, BOH_START, BOH_END, BOH_ESID, BOH_EXE,
BOH_VERSION, BOH_JOB, BOH_HOST, BOH_THREAD
FROM SBS0_ADMIN.BOHEADER
WHERE
boh_datetime > sysdate -5.5/24 and BOH_PACKID = 'SIS_SYNC'
ORDER BY BOH_ID
DESC;">>,
<<"SELECT BOH_ID, BOH_PACKID, BOH_FILESEQ, BOH_DATETIME, BOH_FILENAME,
BOH_RECCOUNT, BOH_DATEFC, BOH_DATELC, BOH_RECCOUNT / ((
BOH_END-BOH_START+0.000001)*24*3600) 'Speed', BOH_ESID
FROM SBS0_ADMIN.BOHEADER
BOHEADER
WHERE BOH_DATETIME > sysdate - 1 AND BOH_PACKID IN ('AAA_MERGE');">>,
<<"SELECT BOH_ID, BOH_PACKID, BOH_DATETIME, BOH_FILENAME, BOH_RECCOUNT,
BOH_RECCOUNT / ((BOH_END-BOH_START)*24*3600 + 1) 'Speed',
(BOH_END-BOH_START)*24*60 'Duration[Min]', BOH_ESID, BOH_END
FROM
SBS0_ADMIN.BOHEADER BOHEADER
WHERE BOH_DATETIME >= sysdate-1 AND BOH_PACKID =
'SIS_SYNC';">>,
<<"SELECT AAASL_BOHID, AAASL_ORDER, AAASL_AAAMTID, AAASL_DATETIME, AAASL_COUNT,
AAASL_AAASPID, AAASL_TITLE, AAASL_LENGTH, AAASL_MESSAGE
FROM SBS0_ADMIN.AAASYNCLOG
AAASYNCLOG
WHERE AAASL_DATETIME >= sysdate - 1/24
ORDER BY AAASL_BOHID,
AAASL_ORDER;">>,
<<"SELECT BOH_ID, BOH_PACKID, BOH_DATETIME, BOH_FILENAME, BOH_RECCOUNT,
BOH_ERRCOUNT, BOH_DATEFC, BOH_DATELC, BOH_RECCOUNT / ((
BOH_END-BOH_START+0.000001)*24*3600) 'Speed', BOH_ESID, BOH_EXE, BOH_VERSION,
BOH_JOB, BOH_HOST, BOH_THREAD
FROM SBS0_ADMIN.BOHEADER
WHERE BOH_DATETIME > sysdate
- 0.2 AND BOH_PACKID <> 'SPTRY';">>,
<<"SELECT TRUNC(BOH_DATEFC,'HH24'), SUM(BOH_RECCOUNT)
FROM SBS0_ADMIN.BOHEADER
WHERE
BOH_DATETIME > sysdate - 2 AND BOH_PACKID IN ('SPTRY') AND BOH_FILENAME IN
('SYNC-SICACHE-NOTIFICATIONS')
GROUP BY trunc(BOH_DATEFC,'HH24')
ORDER BY 1;">>,
<<"SELECT TRUNC(BOH_START), SUM(BOH_RECCOUNT)
FROM SBS0_ADMIN.BOHEADER
WHERE
BOH_DATETIME > sysdate - 10 AND BOH_PACKID IN ('SPTRY') AND BOH_FILENAME LIKE
'%-SICACHE%' AND BOH_FILENAME IN ('SYNC-SICACHE-NOTIFICATIONS')
GROUP BY
trunc(BOH_START)
ORDER BY trunc(BOH_START);">>,
<<"SELECT BOH_ID, BOH_PACKID, BOH_START, BOH_FILENAME, BOH_RECCOUNT, (
BOH_END-BOH_START)*24*3600 'Duration', BOH_RECCOUNT / ((
BOH_END-BOH_START+0.00000001)*24*3600) 'Speed', (BOH_END-BOH_DATELC)*24*3600
'Delay sec', BOH_DATEFC, BOH_DATELC, BOH_ESID
FROM SBS0_ADMIN.BOHEADER
WHERE
BOH_DATETIME > sysdate - 5/24 AND BOH_PACKID IN ('SPTRY') AND BOH_FILENAME LIKE
'%-SICACHE%' AND BOH_FILENAME IN ('SYNC-SICACHE-NOTIFICATIONS');">>,
<<"SELECT TRUNC(BOH_START), SUM(BOH_RECCOUNT)
FROM SBS0_ADMIN.BOHEADER
WHERE
BOH_DATETIME > sysdate - 10 AND BOH_PACKID IN ('SPTRY') AND BOH_FILENAME LIKE
'%-SICACHE%' AND BOH_FILENAME Not In ('SYNC-SICACHE-NOTIFICATIONS')
GROUP BY
trunc(BOH_START)
ORDER BY 1;">>,
<<"SELECT BOH_ID, BOH_PACKID, BOH_START, BOH_FILENAME, BOH_RECCOUNT, (
BOH_END-BOH_START)*24*3600 'Duration', BOH_RECCOUNT / ((
BOH_END-BOH_START+0.00000001)*24*3600) 'Speed', BOH_DATEFC, BOH_DATELC,
BOH_ESID
FROM SBS0_ADMIN.BOHEADER
WHERE BOH_DATETIME > sysdate - 1/24 AND
BOH_PACKID IN ('SPTRY') AND BOH_FILENAME LIKE '%-SICACHE%' AND BOH_FILENAME Not
In ('SYNC-SICACHE-NOTIFICATIONS');">>,
<<"SELECT TAIL.BOH_ID, TAIL.BOH_DATETIME, TAIL.BOH_FILENAME,
TAIL.BOH_RECCOUNT
FROM SBS0_ADMIN.BOHEADER RESTART, SBS0_ADMIN.BOHEADER
TAIL
WHERE RESTART.BOH_DATETIME > sysdate - 100     AND RESTART.BOH_PACKID IN
('SPTRY')     AND RESTART.BOH_FILENAME LIKE 'REFRESH-SICACHE(S)(0-%'     AND
TAIL.BOH_ID >= to_char(to_number(RESTART.BOH_ID)-100)     AND TAIL.BOH_ID <=
RESTART.BOH_ID     AND TAIL.BOH_PACKID IN ('SPTRY')     AND TAIL.BOH_FILENAME
LIKE 'REFRESH-SICACHE(S)%'
ORDER BY TAIL.BOH_ID;">>,
<<"SELECT BOH_ID, BOH_PACKID, BOH_START, BOH_FILENAME, BOH_RECCOUNT, (
BOH_END-BOH_START)*24*3600 'Duration', BOH_RECCOUNT / ((
BOH_END-BOH_START+0.00000001)*24*3600) 'Speed', BOH_DATEFC, BOH_DATELC,
BOH_ESID
FROM SBS0_ADMIN.BOHEADER
WHERE BOH_DATETIME > sysdate-1 AND
BOH_PACKID IN ('SPTRY') AND BOH_FILENAME LIKE '%-SICACHE%';">>,
<<"SELECT BOH_ID, BOH_PACKID, BOH_FILESEQ, BOH_DATETIME, BOH_FILENAME,
BOH_RECCOUNT, BOH_ERRCOUNT, BOH_RECCOUNT / ((
BOH_END-BOH_START+0.000001)*24*3600) 'Speed', BOH_ESID, BOH_HOST,
BOH_THREAD
FROM SBS0_ADMIN.BOHEADER
WHERE BOH_DATETIME > sysdate - 3/24 AND
BOH_PACKID IN ('SUBNOTMGR');">>,
<<"SELECT BOH_ID, BOH_PACKID, BOH_FILESEQ, BOH_DATETIME, BOH_FILENAME,
BOH_RECCOUNT, BOH_ERRCOUNT, BOH_DATEFC, BOH_DATELC, BOH_RECCOUNT / ((
BOH_END-BOH_START+0.000001)*24*3600) 'Speed', BOH_ESID
FROM
SBS0_ADMIN.BOHEADER
WHERE BOH_DATETIME > sysdate - 2 AND BOH_PACKID IN ('UCPMFN');">>,
<<"SELECT BOH_ID, BOH_PACKID, BOH_DATETIME, BOH_FILENAME, BOH_RECCOUNT,
BOH_ERRCOUNT, BOH_RECCOUNT / (( BOH_END-BOH_START+0.000001)*24*3600) 'Speed',
DURATION(BOH_END-BOH_START) \"Duration\", BOH_ESID, BOH_EXE, BOH_VERSION,
BOH_JOB, BOH_HOST, BOH_THREAD
FROM SBS0_ADMIN.BOHEADER
WHERE BOH_DATETIME > sysdate
- 1/24;">>,
<<"SELECT SIC_MSISDN, SIC_ENPLASTATE_SMSC, ENPLASS_ENPLAID, ENPLASS_INSYNC,
ENPLASS_SYNC_DATE, ENPLASS_SYNC_RETRIES, ENPLASS_REFRESH_DATE,
ENPLASS_SYNC_REQUEST_DATE
FROM SICACHE_ENPLASTATE, ENPLASYNCSTATE
WHERE
SICACHE_ENPLASTATE.SIC_MSISDN = ENPLASYNCSTATE.ENPLASS_KEY AND SIC_MSISDN =
?Msisdn;
">>,
<<"SELECT EMPLAL_ID, ENPLAL_ENPLAID, EMPLAL_CODE, EMPLAL_ADDRESS, EMPLAL_ONLINE,
EMPLAL_SYNC_USE, EMPLAL_SYNC_MASTER, ENPLAL_SVRAUTH
FROM ENPLALINK;">>,
<<"SELECT ENPLA_ID, ENPLA_LOGIN, ENPLA_PWD, EMPLAL_ADDRESS
