-define(SBS_SAMPLES, [
%<<"select HIH_ID, HIH_SRCTYPE, HIH_MAPID, HIH_DATETIME, HIH_FILENAME, HIH_RECCOUNT, HIH_ERRCOUNT, HIH_ESID, HIH_DATEFC, HIH_DATELC, HIH_RECCOUNT / (HIH_END - HIH_START + 0.0000001) * 24 * 3600 Speed, (HIH_END - HIH_DATEFC) * 24 * 60 Delay, HIH_EXE, HIH_VERSION, HIH_JOB, HIH_HOST, HIH_THREAD from SBS0_ADMIN.HIHEADER where HIH_DATETIME >= sysdate - 1 / 24 and HIH_MAPID like 'MVP-%' and HIH_SRCTYPE = 'ISRV' order by HIH_START ">>,
%<<"select HBH_ID, HBH_HIHID, HBH_DATETIME, HBH_FILENAME, HBH_RECCOUNT, HBH_PACID from SBS0_ADMIN.HBHEADER where HBH_DATETIME >= sysdate - 1 / 24">>,
%<<"select HIH_ID, HIH_MAPID, HIH_SRCTYPE, HIH_MAPID, HIH_FILESEQ, HIH_DATETIME, HIH_FILENAME, HIH_RECCOUNT, HIH_ESID, HIH_DATEFC, HIH_DATELC, HIH_RECCOUNT / (HIH_END - HIH_START + 0.0000001) * 24 * 3600 Speed, (HIH_END - HIH_DATEFC) * 24 * 60 Delay, HIH_EXE, HIH_VERSION, HIH_JOB, HIH_HOST from SBS0_ADMIN.HIHEADER where HIH_DATETIME >= sysdate - 6 / 12 and HIH_MAPID = 'STAN' order by HIH_START ">>,
%<<"select MACHINE, COUNT(*) from SYS.V_$SESSION where USERNAME in ('SBS0_AAAPROVWS') group by MACHINE order by MACHINE ">>,
%<<"select USERNAME from ALL_USERS">>,
%<<"SELECT TRUNC(ENQ_TIMESTAMP), MSG_STATE, CONSUMER_NAME, COUNT(*)\nFROM\nAQ$SIS_AQ_TABLE\nGROUP BY\nMSG_STATE,\n\n\tTRUNC(ENQ_TIMESTAMP),\n\n\tCONSUMER_NAME\nORDER BY\n1,\n\n\tMSG_STATE,\n\n\tCONSUMER_NAME">>,
%<<"select HIH_ID, HIH_SRCTYPE, HIH_MAPID, HIH_DATETIME, HIH_FILENAME, HIH_RECCOUNT, HIH_ERRCOUNT, HIH_ESID, HIH_DATEFC, HIH_DATELC, HIH_RECCOUNT / (HIH_END - HIH_START + 0.0000001) * 24 * 3600 Speed, (HIH_END - HIH_DATEFC) * 24 * 60 Delay, HIH_JOB, HIH_HOST, HIH_VERSION from SBS0_ADMIN.HIHEADER where HIH_DATETIME >= sysdate - 1 / 24 and HIH_MAPID in ('MMSC') order by HIH_START ">>,
%<<"SELECT TRUNC(BOH_DATEFC,'HH24'), SUM(BOH_RECCOUNT)\nFROM SBS0_ADMIN.BOHEADER\nWHERE\nBOH_DATETIME > sysdate - 2 AND BOH_PACKID IN ('SPTRY') AND BOH_FILENAME IN\n('SYNC-SICACHE-NOTIFICATIONS')\nGROUP BY trunc(BOH_DATEFC,'HH24')\nORDER BY 1">>,
%<<"select HIM_ID, HIM_SRCTYPE, HIM_DEMO, HIM_IDENT, HIM_FILESEQ, HIM_DATETIME, HIM_FILENAME, HIM_FILEDATE, HIM_MAPID, HIM_TRPERIOD, HIM_TRSID, HIM_TRNUMBER, HIM_TRCOMMENT from SBS0_ADMIN.HIMISSING where HIM_DATETIME >= trunc(sysdate) - 4">>,
%<<"SELECT PAC_ID, PAC_NAME, PAC_ESID, PAC_PERIODID, PAC_STARTDAY \"Day\",\nPAC_STARTHOUR \"Hour\", PAC_STARTMINUTE \"Minute\",\nDECODE(PKG_COMMON_PACKING.ISTIMEFORPACKING(PAC_ID),1,'Yes','') \"IsTime\",\nPAC_DATETRY, PAC_DATEDONE, PAC_XPROCID, PAC_CONDITIONALEXEC, PAC_NOTID,\nPAC_OUTPUTDIR, PAC_DEBUG, PAC_FILEMASK, PAC_LTID, PAC_ITID\nFROM\nSBS0_ADMIN.PACKING\nWHERE PAC_ETID IN ('CONSOL') AND PAC_ESID <> 'D'\nORDER BY\nPAC_ID">>,
%<<"select SIC_STYPE, SIC_PAYMENT_TYPE, SIC_PAYMENT_METHOD, SIC_STATUS, COUNT(*) from SICACHE where SIC_STATUS in (0, 1, 3) and SIC_PAYMENT_TYPE <> 1 group by SIC_STYPE, SIC_PAYMENT_TYPE, SIC_PAYMENT_METHOD, SIC_STATUS order by SIC_STYPE, SIC_PAYMENT_TYPE, SIC_PAYMENT_METHOD ">>,
%<<"select BOH_ID, BOH_PACKID, BOH_DATETIME, BOH_FILENAME, BOH_RECCOUNT, BOH_ERRCOUNT, BOH_DATEFC, BOH_DATELC, BOH_RECCOUNT / (BOH_END - BOH_START + 0.000001) * 24 * 3600 Speed, BOH_ESID, BOH_EXE, BOH_VERSION, BOH_JOB, BOH_HOST, BOH_THREAD from SBS0_ADMIN.BOHEADER where BOH_DATETIME > sysdate - 0.2 and BOH_PACKID <> 'SPTRY'">>,
%<<"select BOH_ID, BOH_PACKID, BOH_FILESEQ, BOH_DATETIME, BOH_FILENAME, BOH_RECCOUNT, BOH_DATEFC, BOH_DATELC, BOH_RECCOUNT / (BOH_END - BOH_START + 0.000001) * 24 * 3600 Speed, BOH_ESID from SBS0_ADMIN.BOHEADER where BOH_DATETIME > sysdate - 1 and BOH_FILENAME like 'SP_MECCACHE_%_AGE'">>,
%<<"select HIH_ID, HIH_MAPID, HIH_FILESEQ, HIH_DATETIME, HIH_FILENAME, HIH_RECCOUNT, HIH_ERRCOUNT, HIH_ESID, HIH_DATEFC, HIH_DATELC, HIH_RECCOUNT / (HIH_END - HIH_START + 0.0000001) * 24 * 3600 Speed, (HIH_END - HIH_DATEFC) * 24 * 60 Delay, HIH_VERSION, HIH_HOST, HIH_EXE, (HIH_END - HIH_START) * 14 * 3600 \"(HIH_END-HIH_START)*24*3600\" from SBS0_ADMIN.HIHEADER where HIH_DATETIME >= sysdate - 1 / 144 and HIH_MAPID like 'MB-MO%' order by HIH_START ">>,
%<<"select concat(OWNER,concat('.', TABLE_NAME)) as QUALIFIED_TABLE_NAME from ALL_TABLES where OWNER=user order by TABLE_NAME">>,
%<<"select SADDR, SID, SERIAL#, SCHEMANAME, OSUSER, PROCESS, MACHINE, PROGRAM, LOGON_TIME from SYS.V_$SESSION where SCHEMANAME in ('SBS0_AAAPROVWS') order by MACHINE, LOGON_TIME ">>,
%<<"select AAAT_MSISDN, AAAT_TOPSCM, AAAT_TOPCU, AAAT_CHARGED, AAAT_BARSCM, AAAT_BARCU, AAAT_AAASPID, AAAT_SYNCDATE, AAAT_ESID, AAAT_ACIDCRE, AAAT_DATECRE, AAAT_ACIDMOD, AAAT_DATEMOD, AAAT_CHNGCNT, AAAT_PREPAID, AAAT_EXTID, AAAT_SC from SBS0_ADMIN.AAATRACKING where AAAT_SYNCDATE >= sysdate - 1 / 24 and AAAT_ACIDCRE = 'CCPROV'">>,
%<<"SELECT TRUNC(ENQ_TIMESTAMP), MSG_STATE, CONSUMER_NAME, COUNT(*)\nFROM\nAQ$SUBNOTIN_AQ_TABLE\nGROUP BY\nMSG_STATE,\n\n\tTRUNC(ENQ_TIMESTAMP),\n\n\tCONSUMER_NAME\nORDER BY\n1,\n\n\tMSG_STATE,\n\n\tCONSUMER_NAME">>,
%<<"select BOH_ID, BOH_PACKID, BOH_FILESEQ, BOH_DATETIME, BOH_FILENAME, BOH_RECCOUNT, BOH_DATEFC, BOH_DATELC, BOH_RECCOUNT / (BOH_END - BOH_START + 0.000001) * 24 * 3600 Speed, BOH_ESID from SBS0_ADMIN.BOHEADER BOHEADER where BOH_DATETIME > sysdate - 1 and BOH_PACKID in ('AAA_MERGE')">>,
%<<"SELECT W_MESSAGE, COUNT(*), MIN(W_ERRORTIME), MAX(W_ERRORTIME)\nFROM\nSBS0_ADMIN.WARNING\nWHERE W_ERRORTIME <trunc(sysdate,'MONTH') AND W_ERRORTIME >=\nadd_months(trunc(sysdate,'MONTH'), -1) AND W_APPLIC <> 'CHECK OC' AND W_MESSAGE\nLIKE 'SMSC MEC Processing Error 1000000300%'\nGROUP BY W_MESSAGE\nORDER BY 2\nDESC">>,
%<<"select AAAT_MSISDN, AAAT_TOPSCM, AAAT_TOPCU, AAAT_CHARGED, AAAT_BARSCM, AAAT_BARCU, AAAT_AAASPID, AAAT_SYNCDATE, AAAT_ESID, AAAT_ACIDCRE, AAAT_DATECRE, AAAT_ACIDMOD, AAAT_DATEMOD, AAAT_CHNGCNT, AAAT_PREPAID, AAAT_EXTID, AAAT_SC, AAAT_ABOSTARTDATE, AAAT_ABOSTARTNOTSTATE from SBS0_ADMIN.AAATRACKING where ROWNUM <= 100 and AAAT_TOPSCM >= 500.0000 and AAAT_CHARGED >= 500.0000 order by AAAT_SYNCDATE desc ">>,
%<<"select A.AAAT_MSISDN, A.AAAT_TOPSCM, A.AAAT_TOPCU, A.AAAT_CHARGED, A.AAAT_BARSCM, A.AAAT_BARCU, A.AAAT_AAASPID, A.AAAT_SYNCDATE, A.AAAT_ESID, A.AAAT_ACIDCRE, A.AAAT_DATECRE, A.AAAT_ACIDMOD, A.AAAT_DATEMOD, A.AAAT_CHNGCNT, A.AAAT_PREPAID, A.AAAT_EXTID, A.AAAT_SC from AAATRACKING A where AAAT_MSISDN = '41792256540'">>,
%<<"select W_ERRORTIME, W_APPLIC, W_PROCEDURE, W_TOPIC, W_MESSAGE, W_SHORTID, W_BIHID, W_BOHID, W_BDID from SBS0_ADMIN.WARNING where W_ERRORTIME > sysdate - 1 / 12">>,
%<<"select MSG_STATE, MIN(ENQ_TIME), MAX(ENQ_TIME), COUNT(*) from SBS0_ADMIN.AQ$SUBNOTIN_AQ_TABLE A  group by msg_state order by MSG_STATE ">>,
%<<"select BIVAL_BILLTEXT, BIVAL_SHORTID, BIVAL_REPID, BIVAL_REASON, BIVAL_DATE from SBS0_ADMIN.BILLTEXT_VALIDATION where BIVAL_DATE > sysdate - 10">>,
%<<"SELECT SUBSTR(W_MESSAGE,INSTR(W_MESSAGE,'(')) \"MSISDN\", COUNT(*) \"SMS\",\nMIN(W_ERRORTIME) \"FromDate\", MAX(W_ERRORTIME) \"ToDate\",\nDURATION(MAX(W_ERRORTIME)-MIN(W_ERRORTIME)) \"Duration\"\nFROM\nSBS0_ADMIN.WARNING\nWHERE W_ERRORTIME >= sysdate -365 AND W_TOPIC =\n'M05_PREPOST_LOOKUP'\nGROUP BY SUBSTR(W_MESSAGE,INSTR(W_MESSAGE,'('))\nHAVING\nCOUNT(*) >= 5\nORDER BY 2 DESC">>,
%<<"SELECT SUBSTR(W_MESSAGE,1,4), TRUNC(W_ERRORTIME), COUNT(*)\nFROM\nSBS0_ADMIN.WARNING\nWHERE W_ERRORTIME >= trunc(sysdate) -10 AND W_APPLIC <>\n'CHECK OC' AND W_MESSAGE LIKE '%MEC Processing Error 1000000300:%'\nGROUP BY\nSUBSTR(W_MESSAGE,1,4), TRUNC(W_ERRORTIME)\nORDER BY 1">>,
%<<"select HIH_ID, HIH_SRCTYPE, HIH_MAPID, HIH_FILESEQ, HIH_DATETIME, HIH_FILENAME, HIH_RECCOUNT, HIH_ERRCOUNT, HIH_ESID, HIH_DATEFC, HIH_DATELC, HIH_RECCOUNT / (HIH_END - HIH_START + 0.0000001) * 24 * 3600 Speed, (HIH_END - HIH_DATEFC) * 24 * 60 Delay, HIH_VERSION, HIH_HOST, HIH_EXE, (HIH_END - HIH_START) * 14 * 3600 \"(HIH_END-HIH_START)*24*3600\" from SBS0_ADMIN.HIHEADER where HIH_DATETIME >= sysdate - 1 / 120 and HIH_MAPID like 'MB-C%' order by HIH_START ">>,
%<<"select HIH_ID, HIH_SRCTYPE, HIH_MAPID, HIH_FILESEQ, HIH_DATETIME, HIH_FILENAME, HIH_RECCOUNT, HIH_ERRCOUNT, HIH_ESID, HIH_DATEFC, HIH_DATELC, (HIH_END - HIH_START) * 24 * 3600 Duration, HIH_RECCOUNT / (HIH_END - HIH_START + 0.00001) * 24 * 3600 Speed, (HIH_END - HIH_DATEFC) * 24 * 60 Delay, HIH_EXE, HIH_JOB, HIH_HOST, HIH_VERSION from SBS0_ADMIN.HIHEADER where HIH_DATETIME >= sysdate - 1 / 144 and HIH_SRCTYPE in ('MSC') order by HIH_START ">>,
%<<"select HIH_ID, HIH_SRCTYPE, HIH_MAPID, HIH_DATETIME, HIH_FILENAME, HIH_RECCOUNT, HIH_ERRCOUNT, HIH_ESID, HIH_DATEFC, HIH_DATELC, HIH_RECCOUNT / (HIH_END - HIH_START + 0.0000001) * 24 * 3600 Speed, (HIH_END - HIH_DATEFC) * 24 * 60 Delay, HIH_EXE, HIH_VERSION, HIH_JOB, HIH_HOST, HIH_THREAD from SBS0_ADMIN.HIHEADER where HIH_DATETIME >= sysdate - 2 / 24 order by HIH_START ">>,
%<<"select ENPLASE_KEY, ENPLASE_ENPLAID, ENPLASE_ERRORTIME, ENPLASE_ERRTID, ENPLASE_TOPIC, ENPLASE_MESSAGE, ENPLASE_CLOSETIME from ENPLASYNCERROR  order by ENPLASE_ERRORTIME desc ">>,
%<<"SELECT TRUNC(W_ERRORTIME), W_SHORTID, W_MESSAGE, COUNT(*),\nSUM(\"Offline\"-\"Online\") AS \"Loss\" FROM SBS0_ADMIN.WARNING_ONLINE_CHARGE WHERE\nW_ERRORTIME >= sysdate -3 GROUP BY trunc(W_ERRORTIME), W_SHORTID, W_MESSAGE\nORDER BY W_SHORTID">>,
%<<"select BOH_ID, BOH_PACKID, BOH_START, BOH_FILENAME, BOH_RECCOUNT, (BOH_END - BOH_START) * 24 * 3600 \"Duration\", BOH_RECCOUNT / (BOH_END - BOH_START + 0.00000001) * 24 * 3600 \"Speed\", (BOH_END - BOH_DATELC) * 24 * 3600 \"Delay sec\", BOH_DATEFC, BOH_DATELC, BOH_ESID from SBS0_ADMIN.BOHEADER where BOH_DATETIME > sysdate - 5 / 24 and BOH_PACKID in ('SPTRY') and BOH_FILENAME like '%-SICACHE%' and BOH_FILENAME in ('SYNC-SICACHE-NOTIFICATIONS')">>,
%<<"SELECT PAC_ID, PAC_NAME, PAC_ESID, PAC_PERIODID,\nDECODE(PKG_COMMON_PACKING.ISTIMEFORPACKING(PAC_ID),1,'Yes','') \"IsTime\",\nPAC_DATETRY, PAC_DATEDONE, PAC_CONDITIONALEXEC, PAC_DEBUG, PAC_FILEMASK\nFROM\nSBS0_ADMIN.PACKING\nWHERE PAC_ETID IN ('SPTRY') AND PAC_ESID <> 'D'\nORDER BY PAC_ID">>,
%<<"select HIH_ID, HIH_SRCTYPE, HIH_MAPID, HIH_DATETIME, HIH_FILENAME, HBH_FILENAME from SBS0_ADMIN.HIHEADER, SBS0_ADMIN.HBHEADER where SBS0_ADMIN.HIHEADER.HIH_ID = SBS0_ADMIN.HBHEADER.HBH_HIHID and HIH_DATETIME >= to_date('30.10.2006', 'dd.mm.yyyy') and HIH_SRCTYPE in ('SMSC') and HIH_FILENAME like 'SMCH41-1489' order by HIH_START ">>,
%<<"SELECT MAP_ID, MAP_NAME, MAP_ETID, MAP_SRCTID,\nDECODE(PKG_COMMON_MAPPING.ISTIMEFORMAPPING(MAP_ID),1,'Yes','') \"IsTime\",\nMAP_SOX_CODE, MAP_SOX_NAME, MAP_SOX_REPORT, MAP_EXECUTE, MAP_CONDITIONALEXEC,\nMAP_DEBUG, MAP_ENDCLEARANCE\nFROM MAPPING\nWHERE MAP_EXECUTE <> 0\nORDER BY\nMAP_ID">>,
%<<"select BIH_ID, BIH_SRCTYPE, BIH_MAPID, BIH_FILESEQ, BIH_DATETIME, BIH_FILENAME, BIH_RECCOUNT, BIH_ESID, BIH_DATEFC, BIH_DATELC, BIH_RECCOUNT / (BIH_END - BIH_START + 0.0000001) * 24 * 3600 Speed from biheader where BIH_DATETIME >= sysdate - 24 / 24 order by BIH_START ">>,
%<<"select concat(OWNER,concat('.', VIEW_NAME)) as QUALIFIED_TABLE_NAME from ALL_VIEWS where OWNER=user order by VIEW_NAME">>,
%<<"select HIH_ID, HIH_SRCTYPE, HIH_MAPID, HIH_FILESEQ, HIH_DATETIME, HIH_FILENAME, HIH_RECCOUNT, HIH_ESID, HIH_DATEFC, HIH_DATELC, HIH_RECCOUNT / (HIH_END - HIH_START + 0.0000001) * 24 * 3600 Speed, (HIH_END - HIH_DATEFC) * 24 * 60 Delay, HIH_EXE, HIH_VERSION, HIH_JOB, HIH_HOST from SBS0_ADMIN.HIHEADER where HIH_DATETIME >= sysdate - 1 / 24 and HIH_SRCTYPE in ('VASC') order by HIH_START ">>,
%<<"select SIC_STYPE, SIC_PAYMENT_TYPE, SIC_PAYMENT_METHOD, SIC_STATUS, COUNT(*) from SICACHE where SIC_STATUS in (0, 1, 3) group by SIC_STYPE, SIC_PAYMENT_TYPE, SIC_PAYMENT_METHOD, SIC_STATUS order by SIC_STYPE, SIC_PAYMENT_TYPE, SIC_PAYMENT_METHOD ">>,
%<<"SELECT TRIM(SUBSTR(W_MESSAGE,1,4)) SRCTYPE,\nREPLACE(SUBSTR(W_MESSAGE,INSTR(W_MESSAGE,'(')+1,11),')','') MSISDN, SIC_IMSI,\nSIS_INFO, SIC_INFO, COUNT(*), MIN(W_DATETIME), MAX(W_DATETIME),\nDURATION(MAX(W_DATETIME)-MIN(W_DATETIME)) Duration FROM SBS0_ADMIN.WARNING,\nSICACHE_SIS WHERE  SICACHE_SIS.SIC_MSISDN(+) =\nto_number(REPLACE(SUBSTR(W_MESSAGE,INSTR(W_MESSAGE,'(')+1,11),')','')) AND\nW_MESSAGE Not Like\n'SMSC MEC Processing Error 1000000300:Subscriber Info Not Available (3D%' AND\nW_ERRORTIME >= sysdate-1 AND W_APPLIC <> 'CHECK OC' AND W_TOPIC IN\n('M05_PREPOST_LOOKUP','M14_CHECK_CDR') GROUP BY TRIM(SUBSTR(W_MESSAGE,1,4)),\nREPLACE(SUBSTR(W_MESSAGE,INSTR(W_MESSAGE,'(')+1,11),')',''), SIC_IMSI,\nSIS_INFO, SIC_INFO ORDER BY 1, 2, 3">>,
%<<"select SIC_MSISDN, SIC_IMSI, SIC_PAYMENT_METHOD, SIC_STYPE, SIC_DUOCARD, SIC_DUAL_MSISDN, SIC_DUAL_IMSI, SIC_ENTRY_DATE, SIC_VALID_DATE, SIC_TRANSFER_DATE, SIC_CHANGE_DATE, SIC_SYNC_DATE, SIC_ESID, SIC_STATUS, SIC_PAYMENT_TYPE, SIC_USER_LANGUAGE, SIC_BIRTHDATE, SIC_MAIN_SEGMENT, SIC_BIRTHDATE_STATUS, SIC_BARRING_STATE, SIC_CUSTOMER_ID, SIC_COMPANY_ID, SIC_TOP_LEVEL_COMPANY_ID, SIC_FINE_SEGMENT, SIC_SERVICE_NUMBER, SIC_BRAND, SIC_MODEL, SIC_REFRESH_TYPE, SIC_CHANGECOUNT from SBS0_ADMIN.SICACHE where SIC_PAYMENT_METHOD = 5 and SIC_ENTRY_DATE >= sysdate - 1 / 96">>,
%<<"select W_APPLIC, W_PROCEDURE, W_TOPIC, W_MESSAGE, W_SHORTID, W_BIHID, COUNT(*), MIN(W_BDID) from SBS0_ADMIN.WARNING where W_ERRORTIME < trunc(sysdate) and W_ERRORTIME >= trunc(sysdate) - 1 and W_APPLIC <> 'CHECK OC' group by W_APPLIC, W_PROCEDURE, W_TOPIC, W_MESSAGE, W_BIHID, W_SHORTID order by W_MESSAGE, W_SHORTID, W_BIHID ">>,
%<<"SELECT TRUNC(W_ERRORTIME), COUNT(*)\nFROM SBS0_ADMIN.WARNING\nWHERE W_ERRORTIME\n>= trunc(sysdate) -10 AND W_PROCEDURE <> 'M00_MEC_CDR' AND W_TOPIC IN\n('ONLINE CHARGE ERROR')\nGROUP BY trunc(W_ERRORTIME)\nORDER BY\ntrunc(W_ERRORTIME)">>,
%<<"select TABLE_NAME, INDEX_NAME, TABLESPACE_NAME, INI_TRANS, MAX_TRANS, INITIAL_EXTENT, NEXT_EXTENT, BLEVEL, LEAF_BLOCKS, DISTINCT_KEYS, AVG_LEAF_BLOCKS_PER_KEY, AVG_DATA_BLOCKS_PER_KEY, CLUSTERING_FACTOR, NUM_ROWS, SAMPLE_SIZE, LAST_ANALYZED from ALL_INDEXES where OWNER = 'SBS0'">>,
%<<"select W_ERRORTIME, W_TOPIC, W_MESSAGE, W_SHORTID, SIC_MSISDN, SIS_MSISDN_INFO(SUBSTR(W_MESSAGE, 23, 11)), SIC_PAYMENT_METHOD, SIC_STATUS, SIC_STYPE, SIC_DUOCARD, SIC_DUAL_MSISDN, SIC_DUAL_IMSI, SIC_ENTRY_DATE, SIC_VALID_DATE, SIC_TRANSFER_DATE, SIC_CHANGE_DATE, SIC_SYNC_DATE, SIC_ESID, SIC_PAYMENT_TYPE, SIC_USER_LANGUAGE, SIC_BIRTHDATE, SIC_MAIN_SEGMENT, SIC_BIRTHDATE_STATUS, SIC_BARRING_STATE, SIC_CUSTOMER_ID, SIC_COMPANY_ID, SIC_TOP_LEVEL_COMPANY_ID, SIC_FINE_SEGMENT, SIC_SERVICE_NUMBER, SIC_BRAND, SIC_MODEL, SIC_REFRESH_TYPE from SBS0_ADMIN.WARNING, SICACHE where SIC_MSISDN = to_number(SUBSTR(W_MESSAGE, 23, 11)) and W_ERRORTIME >= sysdate - 4 / 24 and W_TOPIC in ('SSD Cache Alarm')">>,
%<<"select BOH_ID, BOH_PACKID, BOH_DATETIME, BOH_FILENAME, BOH_RECCOUNT, BOH_RECCOUNT / ((BOH_END - BOH_START) * 24 * 3600 + 1) Speed, (BOH_END - BOH_START) * 24 * 60 DurationMin, BOH_ESID, BOH_END from SBS0_ADMIN.BOHEADER BOHEADER where BOH_DATETIME >= sysdate - 1 and BOH_PACKID = 'SIS_SYNC'">>,
%<<"SELECT PAC_ID, PAC_ETID, PAC_NAME, PAC_ESID, PAC_PERIODID, PAC_STARTDAY \"Day\",\nPAC_STARTHOUR \"Hour\", PAC_STARTMINUTE \"Minute\",\nDECODE(PKG_COMMON_PACKING.ISTIMEFORPACKING(PAC_ID),1,'Yes','') \"IsTime\",\nPAC_DATETRY, PAC_DATEDONE, PAC_XPROCID, PAC_FILEMASK, PAC_CONDITIONALEXEC,\nPAC_DEBUG\nFROM SBS0_ADMIN.PACKING\nWHERE PAC_ESID <> 'D'\nORDER BY PAC_ID">>,
%<<"SELECT TRUNC(W_ERRORTIME,'HH24'), W_TOPIC, COUNT(*)\nFROM\nSBS0_ADMIN.WARNING\nWHERE W_ERRORTIME >= sysdate -1 AND W_APPLIC <> 'CHECK OC'\nAND W_TOPIC IN ('SSD Cache Alarm')\nGROUP BY trunc(W_ERRORTIME,'HH24'),\nW_TOPIC\nORDER BY 1">>,
%<<"select 'SBS0_ADMIN.STAG' from DUAL ">>,
%<<"SELECT TRUNC(W_ERRORTIME), COUNT(*)\nFROM SBS0_ADMIN.WARNING\nWHERE W_ERRORTIME\n>= sysdate -30 AND W_PROCEDURE <> 'M00_MEC_CDR' AND W_TOPIC IN\n('ENPLA state corrected') AND W_PROCEDURE IN ('scheduleEnPlaSync') AND W_APPLIC\nIN ('PKG_ENPLA_SYNC_SUB')\nGROUP BY trunc(W_ERRORTIME)\nORDER BY\ntrunc(W_ERRORTIME)">>,
%<<"select BOH_PACKID, BOH_FILENAME, COUNT(*), MIN(BOH_DATETIME), MAX(BOH_DATETIME) from SBS0_ADMIN.BOHEADER where BOH_DATETIME < sysdate and BOH_DATETIME >= sysdate - 8 / 24 group by BOH_PACKID, BOH_FILENAME">>,
%<<"select ENPLASS_ENPLAID, COUNT(*) from ENPLASYNCSTATE where ENPLASS_INSYNC = 0 group by ENPLASS_ENPLAID">>,
%<<"select BOH_ID, BOH_PACKID, BOH_DATETIME, BOH_FILENAME, BOH_RECCOUNT, BOH_ERRCOUNT, BOH_RECCOUNT / (BOH_END - BOH_START + 0.000001) * 24 * 3600 Speed, DURATION(BOH_END - BOH_START) Duration, BOH_ESID, BOH_EXE, BOH_VERSION, BOH_JOB, BOH_HOST, BOH_THREAD from SBS0_ADMIN.BOHEADER where BOH_DATETIME > sysdate - 1 / 24">>,
%<<"select EMPLAL_ID, ENPLAL_ENPLAID, EMPLAL_CODE, EMPLAL_ADDRESS, EMPLAL_ONLINE, EMPLAL_SYNC_USE, EMPLAL_SYNC_MASTER, ENPLAL_SVRAUTH from ENPLALINK ">>,
%<<"select ENPLASS_KEY, ENPLASS_ENPLAID, ENPLASS_INSYNC, ENPLASS_SYNC_RETRIES, ENPLASS_SYNC_DATE, ENPLASS_SYNC_REQUEST_DATE, ENPLASS_ENABLER_ACTIVE from ENPLASYNCSTATE ">>,
%<<"select ENPLA_ID, ENPLA_ALIASID, ENPLA_TYPEID, ENPLA_SYNCHRONIZE, ENPLA_REFRESH, ENPLA_REFRESH_DATE, ENPLA_NEXT_REF_BLOCK, ENPLA_TEST_INSTANCE, ENPLA_INSTANCE_ID, ENPLA_INSTANCE_COUNT, ENPLA_THREAD_COUNT, ENPLA_DEBUG, ENPLA_LOGIN, ENPLA_PWD from ENPLATFORM ">>,
%<<"select ENPLA_ID, ENPLA_ALIASID, ENPLA_TYPEID, ENPLA_SYNCHRONIZE, ENPLA_REFRESH, ENPLA_REFRESH_DATE, ENPLA_NEXT_REF_BLOCK, ENPLA_TEST_INSTANCE, ENPLA_INSTANCE_ID, ENPLA_INSTANCE_COUNT, ENPLA_THREAD_COUNT, ENPLA_DEBUG, ENPLA_LOGIN, ENPLA_PWD from ENPLATFORM where ENPLA_ID in ('SMCH90R', 'SMCH80R', 'SMCH30R') and ENPLA_REFRESH_DATE < sysdate - 4 / 24">>,
